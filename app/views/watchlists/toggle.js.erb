if(true){
  var post_links = document.getElementsByClassName("toggle_note_<%= @note.id %>_watchlisted");

  for (var i = 0; i < post_links.length; i++) {
    <%# 数値を正しいものに更新する %>
    post_links[i].children[0]
            .innerHTML = "<%= escape_javascript(link_to(fa_icon('star', text: @note.watching_users.count ))) %>";

    <% if @watchlist %>
      <%# 該当部分をactiveにする %>
      post_links[i].classList.add('active');
    <% else %>
      <%# 該当部分を非activeにする %>
      post_links[i].classList.remove('active');
    <% end %>
  }
}